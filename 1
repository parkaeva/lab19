#include <iostream>

using namespace std;

int main()
{
    int n, i, j, max, min, t, m, maxi, maxj, mini, minj;
    cin>>n>>m;
    int a[n][m];
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<m+1; j++)
        {
            cout<<"a["<<i<<"]["<<j<<"]=";
            cin>>a[i][j];
        }
    }
    for (i=1; i<n+1; i++)
    {
        max=-1;
        min=1000000;
        for (j=1; j<m+1; j++)
        {
            if (a[i][j]>max)
            {
                max=a[i][j];
                maxi=i;
                maxj=j;
            }
            if (a[i][j]<min)
            {
                min=a[i][j];
                mini=i;
                minj=j;
            }
        }
        t=a[maxi][maxj];
        a[maxi][maxj]=a[mini][minj];
        a[mini][minj]=t;
    }
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<m+1; j++)
        {
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
