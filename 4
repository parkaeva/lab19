#include <iostream>
using namespace std;
int main()
{
    int n, i, j, t, m, k;
    cin>>n>>m;
    int a[n][m];
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<m+1; j++)
        {
            cout<<"a["<<i<<"]["<<j<<"]=";
            cin>>a[i][j];
        }
    }
    for (i=1; i<n; i++)
    {
        for (k=i+1; k<n+1; k++)
        {
            if (a[i][1]>a[k][1])
            {
                for (j=1; j<m+1; j++)
                {
                    t=a[i][j];
                    a[i][j]=a[k][j];
                    a[k][j]=t;
                }
            }
        }
    }
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<m+1; j++)
        {
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
